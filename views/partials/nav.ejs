<nav>
    <ul>
        <li><a href="/"><%= config.site_name %></a></li>
<%      if(config.nav_links !== undefined && config.nav_links !== ''){
          try{
            let links = JSON.parse(config.nav_links)
            links.forEach((link)=>{%>
                <li><a href="<%- link.link %>" rel="noreferrer"><%- link.text %></a></li>
            <%})
          }
        catch (e) {console.error(e.message)}}                           %>
    </ul>
</nav>
