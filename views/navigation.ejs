<!--Navigation Bar here-->
<div class="w3-bar w3-black w3-border w3-card-4">
    <a href="/" class="w3-bar-item w3-button w3-mobile w3-green">Home</a>
    <a href="/user" class="w3-bar-item w3-button w3-mobile">User</a>
    <a href="/post" class="w3-bar-item w3-button w3-mobile">Post</a>
    <div class="w3-dropdown-hover w3-mobile">
        <button class="w3-button">Services</button>
        <div class="w3-dropdown-content w3-bar-block">
            <% if (services.length){ %>
                <% services.forEach((service)=>{ %>
                    <a href="<%= service.link%>" class="random-color w3-bar-item w3-button w3-mobile"><%= service.title%></a>
            <% })} %>
        </div>
    </div>
</div>
